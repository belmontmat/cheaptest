    "@kubernetes/client-node": "^1.4.0",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "glob": "^10.3.10",
    "ora": "^5.4.1",
    "table": "^6.8.1",
    "tar": "^7.5.7",
    "uuid": "^9.0.1",
    "yaml": "^2.3.4"
